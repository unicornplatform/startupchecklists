{
  "name": "startupchecklists",
  "version": "1.0.0",
  "description": "Startup Landing Page Checklists",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "format": "prettier --write src/**/*.{js,json,css,md}",
    "build": "npm run clean && parcel build src/index.html --no-source-maps --no-cache",
    "start": "parcel src/index.html --open --no-cache",
    "watch": "parcel watch src/index.html",
    "lint:es": "eslint --ignore-path .gitignore \"src/**/*.js\"",
    "lint:style": "stylelint \"src/**/*.css\"",
    "lint": "npm run lint:es && npm run lint:style"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unicornplatform/startupchecklists.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unicornplatform/startupchecklists/issues"
  },
  "homepage": "https://github.com/unicornplatform/startupchecklists#readme",
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "babel-core": "^6.26.3",
    "babel-minify": "^0.5.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.5.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "node-sass": "^4.12.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-handlebars": "^0.4.4",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.2",
    "stylelint": "^13.2.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^8.0.1"
  }
}
